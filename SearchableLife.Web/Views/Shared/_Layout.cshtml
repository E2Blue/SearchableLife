<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/metro-colors.css")
</head>
<body class="@ViewBag.WrapClass">
    <div id="header" class="container">
        <div class="span24">
            <h2 id="title" class="blue">Sebastian Rosengren</h2>
            <h3 id="sectiontitle" class="blue">@ViewBag.SectionTitle</h3>
        </div>
        <div class="span24"></div>
    </div>

    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
</body>
</html>
